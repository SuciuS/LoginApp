<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIcon?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.HBox?>

<HBox alignment="CENTER_RIGHT" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controllers.TitleBarController">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <children>
      <FontAwesomeIcon fill="#faeb50" iconName="CIRCLE" onMouseClicked="#min" strokeType="OUTSIDE" strokeWidth="0.0">
         <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </HBox.margin>
      </FontAwesomeIcon>
      <FontAwesomeIcon fill="#4fda4d" iconName="CIRCLE" onMouseClicked="#max" strokeType="OUTSIDE" strokeWidth="0.0">
         <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </HBox.margin>
      </FontAwesomeIcon>
      <FontAwesomeIcon fill="#ff3d4e" iconName="CIRCLE" onMouseClicked="#close" strokeType="OUTSIDE" strokeWidth="0.0">
         <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </HBox.margin>
      </FontAwesomeIcon>
   </children>
</HBox>
